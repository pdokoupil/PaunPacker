<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vw="clr-namespace:PaunPacker.Plugins.Views"
                    xmlns:vm="clr-namespace:PaunPacker.Plugins.ViewModels">

    <DataTemplate DataType="{x:Type vm:FreeRectangleExtractorViewModel}">
        <vw:FreeRectangleExtractorView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:FreeRectangleMergerViewModel}">
        <vw:FreeRectangleMergerView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:FreeRectanglePostProcessorViewModel}">
        <vw:FreeRectanglePostProcessorView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:FreeRectangleSplitterViewModel}">
        <vw:FreeRectangleSplitterView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:RectangleOrientationSelectorViewModel}">
        <vw:RectangleOrientationSelectorView/>
    </DataTemplate>

</ResourceDictionary>